      },
      {
        "pagePath": "pages/order/order",
        "iconPath": "/image/order-notActive.png",
        "selectedIconPath": "/image/order-active.png",
        "text": "订单明细"
      },
      {
        "pagePath": "pages/personal/personal",
        "iconPath": "/image/my-notActive.png",
        "selectedIconPath": "/image/my-active.png",
        "text": "我的"
      }
    ]
  },
  "networkTimeout": {
    "request": 60000,
    "connectSocket": 60000,
    "uploadFile": 60000,
    "downloadFile": 60000
  },
  "sitemapLocation": "sitemap.json",
  "useExtendedLib": {
    "weui": true
  },
  "usingComponents": {
    "mp-loading": "weui-miniprogram/loading/loading",
